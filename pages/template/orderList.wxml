<template name="orderItem">
  <view class="listItme flex-row" style="display: flex;">
      <view class="flex-row flex-view-item" style="display: block;">
        <view class="flex-view-item">{{item.name}}</view>
        <view class="flex-view-item">{{item.time_end}}</view>
      </view>
      
      <view class="flex-view-item itmeView" style="display: block;">
        <view class="flex-view-item itmePrice">ï¿¥{{item.real_price}}</view>
      </view>
  </view>
  <view class="lline"></view>
</template>

<template name="orderList">
    <block wx:for="{{orderList}}">
        <template is="orderItem" data="{{index:index,item:item}}"/>
    </block>
</template>